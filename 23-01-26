 single level inheritence

 code1:-
 
class Vehicle{
    void start(){
        System.out.println("Vehicle Starts");
    }
}
class Bike extends Vehicle{
    void drive(){
        System.out.println("Bike running");
    }
    
}
public class Main {
    public static void main(String[] args) {
        Bike b=new Bike();
        b.start();
        b.drive();
    }
}
